<p id="notice"><%= notice %></p>

<h1>Porters</h1>

<table>
  <thead>
    <tr>
      <th>Host name</th>
      <th>Port number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @porters.each do |porter| %>
      <tr>
        <td><%= porter.host_name %></td>
        <td><%= porter.port_number %></td>
        <td><%= link_to 'Show', porter %></td>
        <td><%= link_to 'Edit', edit_porter_path(porter) %></td>
        <td><%= link_to 'Destroy', porter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Porter', new_porter_path %>
